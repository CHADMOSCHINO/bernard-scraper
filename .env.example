# Bernard Scraper Environment Variables
# Copy this to .env and fill in your values

# Database (Render Postgres / Supabase Postgres / etc)
# Use the "Internal Database URL" on Render when possible
DATABASE_URL=postgresql://user:password@host:5432/dbname

# Optional: API Keys for enrichment
HUNTER_API_KEY=
CLEARBIT_API_KEY=

# Optional: Proxy settings for scraping
PROXY_URL=
PROXY_USERNAME=
PROXY_PASSWORD=
